/* pages/myTeam/index.wxss */
Page {
  background: #f5f5f5;
}
.teams {
  background: #fff;
  margin-top:15rpx;
}
.leftPhoto {
  width:120rpx;
  height:120rpx;
  border-radius: 50%;
  background: #ccc;
}
.leftPhoto image {
  width:100%;
  height:100%;
  border-radius: 50%;
}
.people {
  padding:30rpx;
  display:flex;
}
.right {
  margin-left:30rpx;
  border-bottom: 2rpx solid #f5f5f5;
  width:80%;
}
.name {
  color:#000;
  font-size:36rpx;
  font-weight: 600;
}
.money {
  display:flex;
  margin-top:30rpx;
  margin-bottom: 30rpx;
}
.money view:first-child{
  flex:1;
  margin-right:50rpx;
  color:#666;
  font-size:32rpx;
  font-weight: 500;
}
.money view:last-child{
  flex:1;
  color: #268aed;
  font-size:30rpx;
  font-weight: bold;
}
.money image {
  width:16rpx;
  height:20rpx;
  float: right;
  padding-left:24rpx;
  margin-top:6px;
}