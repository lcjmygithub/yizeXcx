<!--pages/partner/index.wxml-->
<view class="top">
  <view class='mysCon'>
    <view>
      <image src='{{partnerLists.headimgurl}}'></image>
    </view>
    <view class='panerName'>
      <view>{{partnerLists.nickname}}</view>
      <view>成为合伙人: <text>{{distributiontime || 0}}</text></view>
    </view>
  </view>
</view>
<!--客户数据-->
<view class="createTask" bindtap='createTask'>
  <view>
    <view>
      <text>{{partnerLists.total || 0}}</text>元</view>
    <view>总收益</view>
  </view>
  <view>
    <view>
      <text>{{partnerLists.cash || 0}}</text>元</view>
    <view>可提现</view>
  </view>
  <view>
    <view>
      <text>{{partnerLists.today || 0}}</text>元</view>
    <view>今日收益</view>
  </view>
</view>

<view class='myLists'>
  <navigator url="../../pages/myearnings/index" hover-class="none" style=' border-bottom:1rpx solid #e9e1ec;'>
    <view class='myLis'>
      <view>
        <text>我的收益</text>
      </view>
      <view>
        <image style="width:15rpx" src='../../img/arrow_right3.png'></image>
      </view>
    </view>
  </navigator>
  <navigator url="../../pages/myTeam/index" hover-class="none" style=' border-bottom:1rpx solid #e9e1ec;'>
    <view class='myLis'>
      <view>
        <text>我的团队</text>
      </view>
      <view>
        <image style="width:15rpx" src='../../img/arrow_right3.png'></image>
      </view>
    </view>
  </navigator>

  <navigator url="../../pages/erweima/index" hover-class="none">
    <view class='myLis'>
      <view>
        <text>邀请好友</text>
      </view>
      <view>
        <image style="width:15rpx" src='../../img/arrow_right3.png'></image>
      </view>
    </view>
  </navigator>

</view>